{
    "name": "kickflip-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint ./src --ext ts,tsx,js,jsx --report-unused-disable-directives",
        "lint:fix": "npm run lint -- --fix",
        "ci:format": "prettier --check ./src",
        "format": "prettier --write ./src",
        "check": "npm run lint:fix && npm run format",
        "commit": "npm run check && cz",
        "prepare": "cd .. && husky kickFlip-project/.husky",
        "test": "jest"
    },
    "config": {
        "commitizen": {
            "path": "./kickFlip-project/node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.51.4",
        "react-infinite-scroll-component": "^6.1.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.23.1",
        "redux": "^5.0.1",
        "swiper": "^11.1.3"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^15.0.6",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "@vitejs/plugin-react": "^4.2.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.2.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "husky": "^9.0.11",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.2.2",
        "vite": "^5.2.0"
    }
}
